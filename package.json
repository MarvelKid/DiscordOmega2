{
  "name": "discord-bot-template",
  "version": "1.0.0",
  "description": "A Typescript Discord bot template.",
  "main": "dist/Bot.js",
  "scripts": {
    "prettier": "prettier --write \"**/*.{js,ts}\"",
    "dev": "node .",
    "build": "rm -rf dist && tsc",
    "start": "npm run prettier && npm run build && npm run dev",
    "start:pm2": "pm2 start ecosystem.config.json"
  },
  "engines": {
    "node": ">=17.7.2",
    "npm": ">=8.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hauntmore/Discord-Bot-Template.git"
  },
  "keywords": [
    "template",
    "discord",
    "bot",
    "typescript"
  ],
  "author": "Hauntless#3212",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Hauntmore/Discord-Bot-Template/issues"
  },
  "homepage": "https://github.com/Hauntmore/Discord-Bot-Template#readme",
  "dependencies": {
    "@discordjs/rest": "^0.3.0",
    "@typegoose/typegoose": "^9.7.1",
    "chalk": "^4.1.2",
    "common-tags": "^1.8.2",
    "discord.js": "^13.6.0",
    "mongoose": "^6.2.10",
    "prettier": "^2.6.1",
    "sourcebin": "^4.3.5"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "dotenv": "^16.0.0",
    "pm2": "^5.2.0",
    "typescript": "^4.6.3"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.6",
    "erlpack": "github:discord/erlpack",
    "utf-8-validate": "^5.0.9",
    "zlib-sync": "^0.1.7"
  }
}
